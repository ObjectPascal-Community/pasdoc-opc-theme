{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:svelte/recommended",
        "plugin:prettier/recommended"
    ],
    "overrides": [
        {
            "files": [
                "*.svelte"
            ],
            "processor": "svelte/svelte"
        }
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "extraFileExtensions": [
            ".svelte"
        ]
    }
}